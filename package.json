  "bugs": {
    "url": "https://github.com/jonp2020/be-charity-bay/issues"
  },
  "homepage": "https://github.com/jonp2020/be-charity-bay#readme",
  "keywords": [],
  "description": "",
  "dependencies": {
    "aws-sdk": "^2.784.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "email-templates": "^7.1.2",
    "express": "^4.17.1",
    "file-type": "^16.0.1",
    "knex": "^0.21.12",
    "multiparty": "^4.2.2",
    "nodemailer": "^6.4.15",
    "nodemon": "^2.0.6",
    "pg": "^8.4.2",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "jest-sorted": "^1.0.8",
    "supertest": "^6.0.0"
  }
}
